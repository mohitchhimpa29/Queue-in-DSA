#include <iostream>
#include <queue> 

using namespace std;

int main() {
   
    queue<int> q;

    q.push(22);
    q.push(54);
    q.push(84);

    cout << "Front element of the queue is " << q.front() << endl;
    cout << "Size of the queue is " << q.size() << endl;  
    q.pop(); 
    q.pop();

    cout << "Front element of the queue is " << q.front() << endl;
    cout << "Size of the queue is " << q.size() << endl; 
    
    if (q.empty()) {
        cout << "The queue is empty." << endl;
    }
    else{
        cout << "queue is not empty " << endl;
    }

    return 0;
}
